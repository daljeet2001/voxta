{
  "name": "voxta",
  "version": "0.1.0",
  "private": true,
  "type": "module",

"scripts": {
  "dev": "concurrently \"npm run dev:next\" \"npm run dev:ws\"",
  "dev:next": "next dev",
  "dev:ws": "node --loader ts-node/esm server-ws/index.ts",
  "build": "prisma generate && next build",
  "start": "next start",
  "postinstall": "prisma generate"
}
,

"prisma": {
  "seed": "node --loader ts-node/esm prisma/seed.ts"
},


  "dependencies": {
    "@prisma/client": "^6.15.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
